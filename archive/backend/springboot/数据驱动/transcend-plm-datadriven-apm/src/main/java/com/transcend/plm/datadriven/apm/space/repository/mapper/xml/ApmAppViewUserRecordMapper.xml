<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.transcend.plm.datadriven.apm.space.repository.mapper.ApmAppViewUserRecordMapper">

    <resultMap id="BaseResultMap" type="com.transcend.plm.datadriven.apm.space.repository.po.ApmAppViewUserRecord">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="bid" column="bid" jdbcType="VARCHAR"/>
            <result property="spaceAppBid" column="space_app_bid" jdbcType="VARCHAR"/>
            <result property="appViewConfigBid" column="app_view_config_bid" jdbcType="VARCHAR"/>
            <result property="tenantId" column="tenant_id" jdbcType="BIGINT"/>
            <result property="createdBy" column="created_by" jdbcType="VARCHAR"/>
            <result property="showType" column="show_type" jdbcType="VARCHAR"/>
            <result property="createdTime" column="created_time" jdbcType="TIMESTAMP"/>
            <result property="updatedBy" column="updated_by" jdbcType="VARCHAR"/>
            <result property="updatedTime" column="updated_time" jdbcType="TIMESTAMP"/>
            <result property="deleteFlag" column="delete_flag" jdbcType="BIT"/>
            <result property="enableFlag" column="enable_flag" jdbcType="BOOLEAN"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,bid,space_app_bid,
        app_view_config_bid,tenant_id,created_by,
        created_time,updated_by,updated_time,
        delete_flag,enable_flag
    </sql>
    <delete id="delete">
        delete from apm_app_view_user_record where space_app_bid = #{spaceAppBid} and created_by = #{createdBy}
    </delete>
</mapper>
