{
  "name": "qms-ai-config-center-vue3",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 8072",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:dev": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "preview": "vite preview --port 8072",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "clean": "rimraf dist",
    "analyze": "vite build --mode production && npx vite-bundle-analyzer dist"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@types/js-cookie": "^3.0.6",
    "@vueuse/core": "^11.2.0",
    "axios": "^1.10.0",
    "dayjs": "^1.11.5",
    "echarts": "^5.5.0",
    "element-plus": "^2.8.4",
    "js-cookie": "^3.0.5",
    "pinia": "^3.0.3",
    "vue": "^3.5.18",
    "vue-i18n": "^10.0.4",
    "vue-router": "^4.5.1",
    "vxe-table": "^4.6.0",
    "wujie-vue3": "^1.0.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.16.5",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "~10.3.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "typescript": "~5.8.0",
    "vite": "^7.0.6",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vue-tsc": "^3.0.4"
  }
}
